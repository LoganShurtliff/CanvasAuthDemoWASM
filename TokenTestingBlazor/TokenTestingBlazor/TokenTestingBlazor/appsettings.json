{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Canvas": {
    "client_id": "YourOAuthClientID",
    "redirect_uri": "http://localhost:3000/api/auth/callback/Canvas",
