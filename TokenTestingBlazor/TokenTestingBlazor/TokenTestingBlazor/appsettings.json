{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Canvas": {
    "client_id": "YourCanvasOAuthClientID",
    "redirect_uri": "http://localhost:3000/api/auth/callback/Canvas",
    "token_uri": "https://davistech.instructure.com/login/oauth2/token"
  },
  "Azure": {
    "tenant": "YourTenantID",
    "client_id": "YourClientID",
    "redirect_uri": "http://localhost:3000/callback",
    "cosmos_uri": "YourCosmosURI (Include the trailing slash but not the port number (e.g. :443) )"
  }
}
